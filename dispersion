import sys

# 引数なし → エラー終了
if len(sys.argv) <= 1:
    sys.exit(1)

values = []

# 数値変換チェック
for n in sys.argv[1:]:
    try:
        values.append(float(n))
    except ValueError:
        # 数値でない → 出力せず終了コード 1
        sys.exit(1)

count = len(values)
ave = sum(values) / count

# 分散（母分散）
variance = sum((v - ave) ** 2 for v in values) / count

print(variance)

